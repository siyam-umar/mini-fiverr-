<%= form_with model: [service, review] do |f| %>
  <div class="mb-3">
    <%= f.label :rating %>
    <%= f.select :rating, options_for_select(1..5), {}, class: "form-select" %>
  </div>
  <div class="mb-3">
    <%= f.label :comment %>
    <%= f.text_area :comment, class: "form-control" %>
  </div>
  <%= f.submit "Submit Review", class: "btn btn-success" %>
<% end %>
